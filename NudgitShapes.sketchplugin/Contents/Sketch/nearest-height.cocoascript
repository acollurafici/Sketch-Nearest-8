// Increase Height (ctrl e)
/*
  Author: Anthony Collurafici
  Date: October 2015
  Version: 0.1

*/

var onRun = function (context) { 
  var selection = context.selection, 
    layer = selection[0];
    frame = [layer frame],
    width = [frame width],
    height = [frame height];

    if (height % 8 === 0) {
    log("true");
    height = height + 8;
    } else {
    log("false"); 
    height = Math.ceil(height/8.0) * 8;
    }

    
  [[layer frame] setHeight: (height)]
  log(height);
}